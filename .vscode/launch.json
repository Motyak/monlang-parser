            "name": "(gdb) parser test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/test/test_token_lengths.elf",
            "args": ["[test-9731]"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
